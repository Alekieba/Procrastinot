{
	"manifest_version": 2,
	
	"name": "Procrastinot",
	"description": "Don't visit distracting websites.",
	"version": "1.0",
	
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"http://*/*",
		"https://*/*",
		"notifications",
		"alarms",
		"tabs"
	],
	
	"icons": { 
		"16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon.png" 
	},
	
	"background": {
		"page": "background.html"
	},
	
	"browser_action": {
		"default_title":"Procrastinot",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
	
}